    "i18next": "^19.8.2",
    "js-yaml": "^4.1.0",
    "lodash": "4.17.21",
    "moment": "^2.27.0",
    "monaco-editor": "0.30.0",
    "prop-types": "^15.7.2",
    "query-string": "6.9.0",
    "rax": "^1.1.0",
    "react": "^16.3.0",
    "react-cookies": "^0.1.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dnd": "^7.3.2",
    "react-dnd-html5-backend": "^7.2.0",
